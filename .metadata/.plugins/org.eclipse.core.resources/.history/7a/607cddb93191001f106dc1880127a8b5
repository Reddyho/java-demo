package controlflow;

import java.util.Scanner;

public class Greatest2 {

	public static void main(String[] args) {

		int a,b;
		Scanner scan=new Scanner(System.in);
		
		System.out.println("enter 2 numbers:");
		a=scan.nextInt();
		b=scan.nextInt();
		
		if (a>b) {
			System.out.println(a);
		}
		else {
			System.out.println(b);
		}
		
		scan.close();
	}

}
